{
  "name": "publish-util",
  "version": "1.0.1",
  "description": "Some util to clean up package.json for npm publish",
  "main": "lib/index.js",
  "scripts": {
    "test": "tap",
    "prepack": "node bin/publish-util-prepack",
    "postpack": "node bin/publish-util-postpack",
    "postinstall": "env && node lib/postinstall.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jchip/publish-util.git"
  },
  "bin": {
    "publish-util-prepack": "bin/publish-util-prepack.js",
    "publish-util-postpack": "bin/publish-util-postpack.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.com/",
    "access": "public"
  },
  "files": [
    "bin",
    "lib"
  ],
  "keywords": [
    "npm",
    "publish",
    "cleanup",
    "clean",
    "package.json",
    "util"
  ],
  "author": "Joel Chen",
  "license": "Apache-2.0",
  "dependencies": {
    "find-up": "^5.0.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "prettier": "^2.2.1",
    "tap": "^15.0.6"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ]
  }
}
